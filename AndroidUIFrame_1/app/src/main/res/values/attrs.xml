<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DragTopLayout">

        <!--Set the collapse panel offset-->
        <attr name="dtlCollapseOffset"
              format="dimension" />

        <!--Set enable drag over . The default is true-->
        <attr name="dtlOverDrag"
              format="boolean" />

        <!--Open the top view after the drag layout is created. The default value is false-->
        <attr name="dtlOpen"
              format="boolean" />

    </declare-styleable>
</resources>